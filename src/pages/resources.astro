---
import Layout from "../layouts/Layout.astro";
import { Navbar } from "../components/Navbar.jsx";
import { FAQ } from "../components/FAQ.jsx";
import { AppointmentForm } from "../components/AppointmentForm";
import { Footer } from "../components/Footer.jsx";
import { ScrollUpButton } from "../components/ScrollUpButton.jsx";
import "../styles/Theme.css";
import "../styles/Diagonals.css";
import SiteData from "../constants/siteData.json";
---

<Layout
  title="OVERTIME Sports - Resources"
  seo={{
    title: SiteData.pages.resources.title,
    description: SiteData.pages.resources.description,
  }}
>
  <Navbar client:load />
  <FAQ client:load />
  <AppointmentForm client:load />
  <Footer />
  <ScrollUpButton client:load />
</Layout>
